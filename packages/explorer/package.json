{
  "name": "@casfa/explorer",
  "version": "0.1.0",
  "description": "CASFA file explorer React component",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check .",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0"
  },
  "peerDependencies": {
    "@casfa/client": "workspace:*",
    "@casfa/protocol": "workspace:*",
    "@mui/material": "^6.0.0",
    "@mui/icons-material": "^6.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}

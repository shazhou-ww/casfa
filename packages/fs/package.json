{
  "name": "@casfa/fs",
  "version": "0.1.0",
  "description": "CAS filesystem operations on top of StorageProvider + KeyProvider",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"

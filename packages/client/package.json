{
  "name": "@casfa/client",
  "version": "0.0.1",
  "description": "CASFA client library with unified authorization strategies",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts"
    },
    "./auth": {
      "types": "./src/auth/index.ts",
      "import": "./src/auth/index.ts"
    },
    "./api": {
      "types": "./src/api/index.ts",
      "import": "./src/api/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check ."
  },
  "dependencies": {
    "@casfa/cas-uri": "workspace:*",
    "@casfa/client-auth-crypto": "workspace:*",
    "@casfa/delegate-token": "workspace:*",
    "@casfa/protocol": "workspace:*"
  },
  "peerDependencies": {
    "@casfa/storage-core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@casfa/storage-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}

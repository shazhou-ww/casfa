{
  "name": "@casfa/client",
  "version": "0.2.0",
  "description": "CASFA client library with unified authorization strategies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "bun": "./src/types/index.ts",
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./api": {
      "bun": "./src/api/index.ts",
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun ../../scripts/build-pkg.ts",
    "test": "bun run test:unit",
    "test:unit": "bun test src/",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "tsc --noEmit && biome check .",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@casfa/cas-uri": "workspace:*",
    "@casfa/client-auth-crypto": "workspace:*",
    "@casfa/delegate-token": "workspace:*",
    "@casfa/protocol": "workspace:*"
  },
  "peerDependencies": {
    "@casfa/storage-core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@casfa/storage-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
